      "version": "2.0.0",
      "category": "quality",
      "agents": [
        "./plugins/qa-automation-agent/agents/qa-automation.md",
        "./plugins/ux-product-agent/agents/ux-product.md"
      ],
      "keywords": [
        "qa",
